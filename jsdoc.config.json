{
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "destination": "docs",
    "tutorials": "src/tutorials"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "systemName": "Qualified Embed SDK",
    "footer": "<div><br><br><br><br><br><br></div>",
    "copyright": "Qualified.io",
    "includeDate": true,
    "cleverLinks": true,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false
    },
    "path": "ink-docstrap",
    "theme": "lumen",
    "navType": "inline",
    "linenums": true,
    "dateFormat": "MMMM Do YYYY, h:mm:ss a",
    "syntaxTheme": "dark",
    "sort": false
  }
}
