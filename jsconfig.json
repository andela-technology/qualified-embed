{
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "strictBindCallApply": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "checkJs": true,
    "allowJs": true,
    "declaration": true,
    "target": "ES2019",
    "module": "ESNext",
    "moduleResolution": "node",
    "outDir": "dist",
    "types": []
  },
  "include": [
    "./src/*.js"
  ],
  "verbose": true
}
